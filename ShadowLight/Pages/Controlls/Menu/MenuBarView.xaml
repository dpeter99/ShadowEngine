<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShadowLight.Pages.Controlls.Menu"
             xmlns:System="clr-namespace:System;assembly=mscorlib"
             xmlns:s="https://github.com/canton7/Stylet"
             x:Class="ShadowLight.Pages.Controlls.Menu.MenuBarView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignData local:MenuBarViewModel}">
    <Menu>
        <MenuItem Header="File">
            <MenuItem Header="Open project" Icon="{DynamicResource OpenProjectIcon}" Command="{s:Action OpenProject}"/>
        </MenuItem>
    </Menu>
</UserControl>
